<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.papertok.logrotate</string>

    <key>ProgramArguments</key>
    <array>
      <string>/bin/bash</string>
      <string>/Users/gwaanl/papertok-deploy/current/ops/run_logrotate.sh</string>
    </array>

    <key>StartCalendarInterval</key>
    <dict>
      <key>Hour</key>
      <integer>4</integer>
      <key>Minute</key>
      <integer>30</integer>
    </dict>

    <key>StandardOutPath</key>
    <string>/Users/gwaanl/papertok-deploy/current/data/logs/logrotate.out.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/gwaanl/papertok-deploy/current/data/logs/logrotate.err.log</string>

    <key>RunAtLoad</key>
    <false/>
  </dict>
</plist>
